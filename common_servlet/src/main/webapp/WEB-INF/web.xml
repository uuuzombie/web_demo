<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Common Servlet</display-name>



    <servlet>
        <servlet-name>secret_key</servlet-name>
        <servlet-class>com.sky.demo.common_servlet.servlet.SecretKeyServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>secret_key</servlet-name>
        <url-pattern>/secret</url-pattern>
    </servlet-mapping>

</web-app>
